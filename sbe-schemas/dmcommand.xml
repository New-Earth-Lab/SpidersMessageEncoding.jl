<?xml version="1.0" encoding="UTF-8"?>
<sbe:messageSchema xmlns:sbe="http://fixprotocol.io/2016/sbe"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    package="dmcommand"
    id="4"
    version="0"
    semanticVersion="5.2"
    description="Schema for DM command messages"
